<script>(function($){
  $(function(){
    var shrtr = $("#shrtr"),
        spinner = $("img").attr("src", "/img/spinner.gif");
    shrtr.append(spinner);
    // we want to extned an url so display resolved and redirect
    $.post("/extn", window.shrtrSurl, function(data){
      spinner.hide();
      shrtr.html(data["url"]);
    });
  });  
})(jQuery);
</script>
